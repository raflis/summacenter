                &:focus
                    box-shadow unset
        .modal-body
            iframe
                width 100%
            video
                width 100%

@keyframes spin
    from
        transform translate(-0%, -50%) rotate(0deg)
    to
        transform translate(-0%, -50%) rotate(360deg)

